{
  "name": "git-gatsby",
  "private": true,
  "description": "LaunchDarkly's Docs Site",
  "version": "1.0.0",
  "author": "Yus Ng",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "1.5.1",
    "@theme-ui/components": "^0.2.49",
    "@types/pluralize": "0.0.29",
    "gatsby": "^2.18.0",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-manifest": "^2.2.29",
    "gatsby-plugin-mdx": "^1.0.57",
    "gatsby-plugin-offline": "^3.0.22",
    "gatsby-plugin-react-helmet-async": "^1.0.11",
    "gatsby-plugin-sharp": "^2.3.2",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-source-filesystem": "^2.1.38",
    "gatsby-transformer-sharp": "^2.3.5",
    "pluralize": "8.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "remark-slug": "5.1.2",
    "theme-ui": "^0.2.49"
  },
  "devDependencies": {
    "@svgr/cli": "^4.3.3",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.23",
    "@types/react-helmet": "^5.0.14",
    "@types/react-helmet-async": "^1.0.3",
    "@types/theme-ui": "^0.2.4",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "gatsby-plugin-typescript": "^2.1.19",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.26",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,json,mdx,yml}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "tsc": "tsc",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "svgr": "svgr assets/icons -d src/components/icons --ext tsx --template src/components/icons/svgrTemplate.js --filename-case camel --svgo-config svgo-config.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/launchdarkly/git-gatsby"
  },
  "bugs": {
    "url": "https://github.com/launchdarkly/git-gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn lint && yarn tsc"
    }
  }
}
